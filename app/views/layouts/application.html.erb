<!DOCTYPE html>
<html>
<head>
  <title>Sahc</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <%= breadcrumbs(:style => :bootstrap) %>

    <% flash.each do |flash_type, message| %>
      <div class="alert <%= bootstrap_class_for(flash_type) %>">
        <%= message.html_safe %>
      </div>
    <% end %>

    <%= yield %>
  </div>
</body>
</html>
